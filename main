#include<map>
#include<vector>
#include<string>
#include<iostream>
#include<memory>
#include<sstream>
#include<iomanip>
using namespace std;

enum class FigureType{
    Triangle,
    Rect,
    Circle
};

class Figure{
public:
virtual string Name()const=0;
virtual int Perimeter()const=0;
virtual int Area()const=0;
};

class Triangle:public Figure{
    
public:    
Triangle(const string& name,int a,int b,int c):name_(name),a_(a),b_(b),c_(c){}
virtual string Name()const override{return name_;}
virtual int Perimeter()const override{return a_+b_+c_;}
virtual int Area()const override{return (Perimeter()*(Perimeter()-a_)*(Perimeter()-b_)*(Perimeter()-c_));}

private:
const string name_;
const int a_,b_,c_;

};


class Rect:public Figure{
    
public:
Rect(const string& name,int width, int height):name_(name),width_(width),height_(height){}
virtual string Name()const override{return name_;}
virtual int Perimeter()const=0;
virtual int Area()const=0;

private:
const string name_;
int width_, height_; 


};
class Circle:public Figure{
    
public:    
virtual string Name()const override{return name_;}
virtual int Perimeter()const=0;
virtual int Area()const=0;

private:
const string name_;    

};

shared_ptr<Figure> CreateFigure(istream & is){
    string figure;
    is>>figure;
    if (figure=="Triangle"){
        int a,b,c;
        is>>a,b,c;
        return make_shared<Triangle>(figure,a,b,c);
        
